[init]
	defaultBranch = main
[user]
	name = phnaharris
	email = phnanh.harris@gmail.com
[diff]
    tool = nvimdiff
[merge]
    tool = nvimdiff
[alias]
    a = add
    br = branch
    c = commit -m
    ca = commit --amend
    co = checkout
    commit = cz
    d = diff
    hist = log --pretty=format:\"%Cgreen%h %Creset%cd %Cblue[%cn] %Creset%s%C(yellow)%d%C(reset)\" --graph --date=relative --decorate --all
    llog = log --graph --name-status --pretty=format:\"%C(red)%h %C(reset)(%cd) %C(green)%an %Creset%s %C(yellow)%d%Creset\" --date=relative
    open = "!hub browse"
    st = status
    rs = restore
    rss = restore --staged
    rmv = remote -v
    rma = remote add origin
    rms = remote set-url origin
    wta = worktree add
    wtl = worktree list
    wtr = worktree remove
    wtp = worktree prune
[remote]
	pushDefault = origin
