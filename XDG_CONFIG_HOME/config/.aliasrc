alias g='git'
alias tmx='tmuxinator'
alias de_arch_virt='docker exec -it --user phnaharris arch_virt bash'
alias update-grub='grub-mkconfig -o /boot/grub/grub.cfg'
alias pacfind="pacman -Slq | fzf --multi --preview 'pacman -Si {1}' | xargs -ro sudo pacman -S"
alias parufind="paru -Slq | fzf --multi --preview 'paru -Si {1}' | xargs -ro paru -S"
alias swd="sudo wg-quick down wg1"
alias swu="sudo wg-quick up wg1"

alias sa='source active-venv'
