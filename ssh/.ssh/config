Host github.com
    HostName github.com
    IdentityFile ~/.ssh/id_ed25519_github_phnaharris

Host gitlab.com
    HostName gitlab.com
    IdentityFile ~/.ssh/id_ed25519_gitlab_phnaharris

Host * 
    User phnaharris
    PreferredAuthentications publickey
    IdentitiesOnly=yes
